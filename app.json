{
  "expo": {
    "name": "EchoNote",
    "slug": "EchoNote",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/use/icon.png",
    "scheme": "EchoNote",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/use/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBFF62"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/use/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bossbeaver.EchoNote"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/use/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c8750357-11b9-4212-bf92-252472d08c00"
      }
    },
    "owner": "bossbeaver"
  }
}
